    "@typescript-eslint/parser": "^3.8.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "ts-loader": "^8.0.12",
    "typescript": "^3.9.4",
    "vscode-test": "^1.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
