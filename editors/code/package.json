{
  "name": "vscode-unflow",
  "description": "Unflow is a DSL to convert design to code.",
  "author": "Phodal Huang <h@phodal.com>",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/inherd/unflow"
  },
  "publisher": "Phodal Huang <h@phodal.com>",
  "categories": [],
  "keywords": [],
  "engines": {
    "vscode": "^1.44.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [],
    "configuration": {}
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
